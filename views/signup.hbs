<!DOCTYPE html> 
  <div id="content">
    <div class="container-fluid" "landing-background">
        <nav class="navbar navbar-collapse navbar-expand-lg mr-auto" id="landing-nav">
            <div class="navbar-header" id="landing-header">
                
                <a href="/landing">
                    <h2 class="navbar-brand"><img id="tatlogo" src="/Images/TATLogoDone1.png"></h2>
                     
                </a>
            </div>
            
            <!-- Collapse button -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="input-group collapse navbar-collapse" id="navbarSupportedContent">
                <form id="form-search" method="get" action="" role="form" class="form-inline">
                    <div class="form-group has-feedback">
                        <div class="input-group">
                            <input style="color: #e7e7e7 !important;" type="text" class="form-control form-inline" id="explore" placeholder="Explore" id="search-text-input">
                            <span class="input-group-btn">
                                <button id="letsgo" class="btn btn-default waves-effect" type="button">Let's Go!
                                    <span>
                                        <i class="fa fa-paper-plane-o ml-1"></i>
                                    </span>
                                </button>
                            </span>
                        </div>
                    </div>   
                </form>
               
            </div>
             <!-- Register for account button -->
             <a href="/signup">
                <button class="btn btn-default" id="create-profile" type="button">Create A Profile
                    <span>
                        <i class="fa fa-user fa-2x"></i>
                    </span>
                </button></a>
                <!-- login to your account button-->
                <a href="/signin">
                <button class="btn btn-default" id="yourTravels" type="button">Your Travels
                    <span>
                        <i class="fa fa-plane fa-2x ml-3"></i>
                    </span>
                </button></a>
        </nav>
    </div>
<div class="container-fluid landing-background">
    <div class="row">
        <div class="col-xs-8 col-xs-offset-2 col-sm-6 col-sm-offset-3 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-2 col-xl-8 col-xl-offset-1">
            <div class="panel panel-default" id="create-profile-panel">
                <div class="panel panel-header text-center" id="create-profile-header">
                    Create Your Profile
                </div>
                <div class="panel panel-body" id="profile-panel-body">
                    <form action="/profile/:id" role="form" class="create-user">
                        <div class="md-form">
                            <i class="fa fa-id-card prefix grey-text"></i>
                            <input type="text" class="form-control" id="name" placeholder="Name" required>
                            <label for="name">
                               
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-envelope prefix grey-text"></i>
                            <input type="email" class="form-control" id="email" placeholder="Email Address" required>
                            <label for="email">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-map-marker prefix grey-text"></i>
                            <input type="text" class="form-control" id="location" placeholder="Enter Your Location" required>
                            <label for="location">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-user prefix grey-text"></i>
                            <input type="text" class="form-control" id="handle" placeholder="Enter A Handle" required>
                            <label for="handle">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-qrcode prefix grey-text"></i>
                            <input type="password" class="form-control" id="password1" placeholder="Please Enter A Password" required>
                            <label for="password1">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-thumbs-o-up prefix grey-text"></i>
                            <input type="password" class="form-control" id="password2" placeholder="Please Confirm Your Password" required>
                            <label for="password2">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-photo suffix grey-text"></i>
                            <input type="file" name="profile-photo" id="profile-input" accept="image/*">
                            <label for="profile-photo">
                                
                            </label>
                        </div>
                        <div class="md-form">
                            <button type="button" class="btn btn-default" id="creating-profile">Submit
                                <span>
                                    <i class="fa fa-sun-o ml-1"></i>
                                </span>
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
